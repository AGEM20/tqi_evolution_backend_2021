-- Database: CreditSystemTQIdb

-- DROP DATABASE IF EXISTS "CreditSystemTQIdb";

CREATE DATABASE "CreditSystemTQIdb"
    WITH 
    OWNER = postgres
    ENCODING = 'UTF8'
    LC_COLLATE = 'Portuguese_Brazil.1252'
    LC_CTYPE = 'Portuguese_Brazil.1252'
    TABLESPACE = pg_default
    CONNECTION LIMIT = -1;

COMMENT ON DATABASE "CreditSystemTQIdb"
    IS 'banco de dados analise de credido';